<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.studio.yishujutan.dao.CollectDao">

    <select id="getCircleIds" parameterType="String" resultType="String">
        select object_id from collect where collect_object = 'circle' and user_id = #{user_id};
    </select>

</mapper>